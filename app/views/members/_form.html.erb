<ul>
<% @member.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
<% end %>
</ul>

<%= form_for(  [@company, @member]) do |f| %>

  Company: <%= @company.name %>
  <br>

  <input type="hidden" name="member[company_id]" value="<%= @company.id %>" />

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>

  <%= f.submit %>

<% end %>
